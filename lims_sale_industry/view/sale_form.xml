        expr="/form/notebook/page[@id='other']/field[@name='send_email']"
        position="after">
        <label name="lubrication_plan"/>
        <field name="lubrication_plan"/>
    </xpath>
    <xpath expr="/form/notebook" position="inside">
        <page string="Industry" id="industry">
            <label name="label_from"/>
            <field name="label_from"/>
            <label name="label_to"/>
            <field name="label_to"/>
            <field name="plants" colspan="4" mode="tree"/>
            <field name="equipments" colspan="4" mode="tree"/>
            <field name="components" colspan="4" mode="tree"/>
            <field name="contacts" colspan="4" mode="tree"/>
        </page>
    </xpath>
</data>
