<?xml version="1.0"?>
<data>
    <xpath
        expr="/form/group[@id='header']/label[@name='code']"
        position="replace">
        <label name="fantasy_name"/>
    </xpath>
    <xpath
        expr="/form/group[@id='header']/field[@name='code']"
        position="replace">
        <field name="fantasy_name"/>
    </xpath>
    <xpath
        expr="/form/group[@id='header']/group[@id='checkboxes']/label[@name='active']"
        position="before">
        <label name="code"/>
        <field name="code" xexpand="0"/>
    </xpath>
    <xpath expr="/form/group[@id='checkboxes']" position="inside">
        <label name="complete_file"/>
        <field name="complete_file" xexpand="0" width="25"/>
        <label name="is_invoice_party"/>
        <field name="is_invoice_party" xexpand="0" width="25"/>
    </xpath>
    <xpath expr="/form/notebook/page[@id='stock']" position="after">
        <page id="plants" string="Plants">
            <field name="plants" colspan="4"/>
        </page>
    </xpath>
</data>
