<?xml version="1.0"?>
<form>
    <group id="name_header" colspan="4" col="6">
        <label name="name"/>
        <field name="name"/>
        <label name="alias"/>
        <field name="alias"/>
        <label name="evaluation_order"/>
        <field name="evaluation_order" xexpand="0"/>
    </group>
    <group id="type" colspan="4" col="4">
        <label name="type_"/>
        <field name="type_"/>
        <label name="related_model"/>
        <field name="related_model"/>
    </group>
    <group id="expression" colspan="4" col="3">
        <label name="expression"/>
        <field name="expression" xexpand="1"/>
        <image name="expression_icon" xexpand="0" size="30"/>
    </group>
    <group id="domain_value" colspan="4" col="4">
        <label name="domain"/>
        <field name="domain" colspan="3" widget="pyson"/>
    </group>
    <group id="fixed_value_field" colspan="4" col="4">
        <label name="is_fixed_value"/>
        <field name="is_fixed_value" xexpand="0"/>
        <label name="fixed_value"/>
        <field name="fixed_value"/>
    </group>
    <separator id="storage" colspan="4" string="Storage:"/>
    <group id="storage_field" colspan="4" col="4">
        <label name="transfer_field"/>
        <field name="transfer_field" xexpand="0"/>
        <label name="related_line_field"/>
        <field name="related_line_field"/>
    </group>
    <separator id="import" colspan="4" string="Import:"/>
    <group id="import_spreadsheet" colspan="4" col="6">
        <label name="singleton"/>
        <field name="singleton"/>
        <label name="source_column"/>
        <field name="source_column"/>
        <label name="source_row"/>
        <field name="source_row"/>
    </group>
    <label name="source_start"/>
    <field name="source_start"/>
    <label name="source_end"/>
    <field name="source_end"/>
</form>
