<data>
    <xpath
        expr="/form/notebook/page[@id='general']"
        position="after">
        <page string="Mail Acknowledgment Report" id="mail_ack_report">
            <separator string="Email subject of Acknowledgment of results report"
                colspan="4" id="mail_ack_report_subject"/>
            <field name="mail_ack_report_subject" colspan="4"/>
