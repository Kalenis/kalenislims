<?xml version="1.0"?>
<data>
    <xpath
        expr="/form/notebook/page[@id='general']/label[@name='product']"
        position="before">
        <label name="product_type"/>
        <field name="product_type"/>
        <label name="matrix"/>
        <field name="matrix"/>
        <label name="analysis"/>
        <field name="analysis"/>
    </xpath>
    <xpath
        expr="/form/notebook/page[@id='general']/field[@name='shipping_date']"
        position="after">
        <label name="expiration_date"/>
        <field name="expiration_date"/>
        <label name="purchase_order"/>
        <field name="purchase_order"/>
        <label name="unlimited_quantity"/>
        <field name="unlimited_quantity"/>
        <label name="print_price"/>
        <field name="print_price"/>
    </xpath>
</data>
