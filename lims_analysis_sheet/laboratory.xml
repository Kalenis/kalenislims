<?xml version="1.0"?>
<tryton>
    <data>

<!-- Laboratory Device -->

        <record model="ir.ui.view" id="lab_device_view_form">
            <field name="model">lims.lab.device</field>
            <field name="inherit" ref="lims.lims_lab_device_view_form"/>
            <field name="name">lab_device_form</field>
        </record>

<!-- Device Constant -->

        <record model="ir.ui.view" id="lab_device_constant_view_form">
            <field name="model">lims.lab.device.constant</field>
            <field name="type">form</field>
            <field name="name">lab_device_constant_form</field>
        </record>
        <record model="ir.ui.view" id="lab_device_constant_view_list">
            <field name="model">lims.lab.device.constant</field>
            <field name="type">tree</field>
            <field name="name">lab_device_constant_list</field>
        </record>

<!-- Notebook Rule -->

        <record model="ir.ui.view" id="notebook_rule_view_form">
            <field name="model">lims.rule</field>
            <field name="inherit" ref="lims.notebook_rule_view_form"/>
            <field name="name">notebook_rule_form</field>
        </record>
        <record model="ir.ui.view" id="notebook_rule_view_list">
            <field name="model">lims.rule</field>
            <field name="inherit" ref="lims.notebook_rule_view_list"/>
            <field name="name">notebook_rule_list</field>
        </record>

    </data>
</tryton>
