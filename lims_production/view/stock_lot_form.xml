            <label name="shelf_life_expiration_date"/>
            <field name="shelf_life_expiration_date"/>
            <label name="expiration_date"/>
            <field name="expiration_date"/>
            <group colspan="2" id="dates2">
                <label name="reception_date"/>
                <field name="reception_date"/>
                <label name="preparation_date"/>
                <field name="preparation_date"/>
            </group>
        </group>
        <newline/>
        <label name="common_name"/>
        <field name="common_name"/>
        <label name="chemical_name"/>
        <field name="chemical_name"/>
        <label name="commercial_name"/>
        <field name="commercial_name"/>
        <label name="cas_number"/>
        <field name="cas_number"/>
        <label name="commercial_brand"/>
        <field name="commercial_brand"/>
        <label name="catalog"/>
        <field name="catalog"/>
        <label name="purity_degree"/>
        <field name="purity_degree"/>
        <label name="exclusive_glp"/>
        <field name="exclusive_glp"/>
        <label name="technician"/>
        <field name="technician"/>
        <label name="solvent"/>
        <field name="solvent"/>
    </xpath>
</data>
