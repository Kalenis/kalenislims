<?xml version="1.0"?>
<data>
    <xpath expr="/form/label[@name='shelf_life_expiration_date']"
        position="replace"/>
    <xpath expr="/form/field[@name='shelf_life_expiration_date']"
        position="replace"/>
    <xpath expr="/form/label[@name='expiration_date']" position="replace"/>
    <xpath expr="/form/field[@name='expiration_date']" position="replace"/>
