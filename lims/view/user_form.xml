<?xml version="1.0"?>
<data>
    <xpath expr="/form/notebook/page[@id='preferences']" position="after">
        <page string="LIMS" id="lims">
            <label name="notebook_view"/>
            <field name="notebook_view"/>
            <field name="departments" colspan="4"/>
            <field name="laboratories" colspan="4"/>
            <label name="laboratory"/>
            <field name="laboratory"  widget="selection"/>
            <label name="printer"/>
            <field name="printer"/>
        </page>
    </xpath>
    <xpath expr="/form/notebook/page[@id='user']/separator[@name='signature']" position="after">
        <field colspan="4" xfill="0" xexpand="1" name="signature_image" height="200" width="200" widget="image"/>
    </xpath>
</data>
