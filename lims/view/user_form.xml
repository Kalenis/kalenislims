<?xml version="1.0"?>
<data>
    <xpath
        expr="/form/notebook/page[@id='preferences']"
        position="after">
        <page string="Lims" id="lims">
            <label name="role"/>
            <field name="role"/>
            <label name="notebook_view"/>
            <field name="notebook_view"/>
            <field name="departments" colspan="4"/>
            <field name="laboratories" colspan="4"/>
            <label name="laboratory"/>
            <field name="laboratory"  widget="selection"/>
            <label name="printer"/>
            <field name="printer"/>
        </page>
    </xpath>
</data>
