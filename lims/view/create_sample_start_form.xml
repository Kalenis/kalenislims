            <label name="size_uom"/>
            <field name="size_uom"/>
            <label name="zone"/>
            <field name="zone"/>
            <label name="restricted_entry"/>
            <field name="restricted_entry"/>
            <group col="2" colspan="2" id="storage_time">
                <label name="storage_time"/>
                <field name="storage_time"/>
            </group>
            <group col="4" colspan="2" id="storage_time">
                <label name="trace_report"/>
                <field name="trace_report"/>
                <label name="shared"/>
                <field name="shared"/>
            </group>
            <newline/>
            <group id="labels" string="Labels" col="1" colspan="2" yexpand="1" yfill="1">
                <field name="labels" colspan="2"/>
            </group>
            <field name="services" colspan="2"/>
        </page>
        <page id="comments" string="Comments">
            <separator string="Report comments" colspan="4" id="report_comments"/>
            <field name="report_comments" colspan="4"/>
            <separator string="Comments" colspan="4" id="comments"/>
            <field name="comments" colspan="4"/>
        </page>
    </notebook>
</form>
