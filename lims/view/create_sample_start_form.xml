<?xml version="1.0"?>
<form>
    <notebook width="1200" height="550">
        <page id="general" string="General">
            <label name="date"/>
            <field name="date"/>
            <label name="storage_location"/>
            <field name="storage_location"/>
            <label name="product_type"/>
            <field name="product_type"/>
            <label name="matrix"/>
            <field name="matrix"/>
            <label name="variety"/>
            <field name="variety"/>
            <label name="producer"/>
            <field name="producer"/>
            <group col="6" colspan="4" id="obj_description">
                <label name="obj_description"/>
                <field name="obj_description"/>
                <label name="obj_description_manual"/>
                <field name="obj_description_manual"/>
                <label name="obj_description_manual_eng"/>
                <field name="obj_description_manual_eng"/>
            </group>
            <group col="2" colspan="2" id="sample_client_description">
                <label name="sample_client_description"/>
                <field name="sample_client_description"/>
            </group>
            <group col="2" colspan="2" id="sample_client_description_eng">
                <label name="sample_client_description_eng"/>
                <field name="sample_client_description_eng"/>
            </group>
            <label name="fraction_type"/>
            <field name="fraction_type"/>
            <label name="fraction_state"/>
