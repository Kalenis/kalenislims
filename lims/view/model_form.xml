<?xml version="1.0"?>
<data>
    <xpath
        expr="/form/separator[@name='info']"
        position="replace">
            <newline/>
            <separator name="info" colspan="3"/>
            <separator name="docs" colspan="3"/>
    </xpath>
    <xpath
        expr="/form/field[@name='info']"
        position="replace">
            <field name="info" colspan="3"/>
            <field name="docs" colspan="3"/>
    </xpath>
</data>
