<?xml version="1.0"?>
<form>
    <label name="number"/>
    <field name="number"/>
    <group col="4" colspan="2" id="entry">
        <label name="entry"/>
        <field name="entry"/>
        <label name="entry_view"/>
        <field name="entry_view"/>
    </group>
    <label name="party"/>
    <field name="party" colspan="3"/>
    <notebook>
        <page string="General" id="general">
            <label name="date"/>
            <field name="date"/>
            <label name="producer"/>
            <field name="producer"/>
            <label name="label"/>
            <field name="label"/>
            <label name="sample_client_description"/>
            <field name="sample_client_description"/>
            <label name="product_type"/>
            <field name="product_type"/>
            <label name="matrix"/>
            <field name="matrix"/>
            <label name="obj_description"/>
            <field name="obj_description"/>
            <label name="obj_description_manual"/>
            <field name="obj_description_manual"/>
            <label name="packages_string"/>
            <field name="packages_string"/>
            <label name="restricted_entry"/>
            <field name="restricted_entry"/>
            <label name="zone"/>
            <field name="zone"/>
            <label name="trace_report"/>
            <field name="trace_report"/>
            <label name="variety"/>
            <field name="variety"/>
            <field name="fractions" colspan="4"/>
        </page>
        <page string="Packages" id="packages">
            <field name="packages" colspan="4"/>
        </page>
        <page string="Attributes" id="attributes">
            <field name="attributes" colspan="4"/>
        </page>
        <page string="Report" id="report">
            <separator string="Report comments" colspan="4" id="report_comments"/>
            <field name="report_comments" colspan="4"/>
        </page>
        <page string="Comments" id="comments">
            <separator string="Comments" colspan="4" id="comments"/>
            <field name="comments" colspan="4"/>
        </page>
        <page string="Resampling" id="resampling">
            <field name="resampling" colspan="4"/>
        </page>
    </notebook>
    <group colspan="4" id="button_confirm">
        <field name="button_confirm_available" invisible="1"/>
        <button name="confirm" string="Confirm"/>
    </group>
</form>
