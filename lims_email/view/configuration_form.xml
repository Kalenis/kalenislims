<?xml version="1.0"?>
<data>
    <xpath expr="/form/notebook/page[@id='general']" position="after">
        <page string="Mail Acknowledgment Report" id="mail_ack_report">
            <label name="result_report_format"/>
            <field name="result_report_format"/>
            <label name="mail_ack_report_grouping"/>
            <field name="mail_ack_report_grouping"/>
            <label name="mail_ack_report_smtp"/>
            <field name="mail_ack_report_smtp"/>
            <label name="mail_ack_report_hide_recipients"/>
            <field name="mail_ack_report_hide_recipients"/>
            <separator string="Email subject of Acknowledgment of results report"
                colspan="4" id="mail_ack_report_subject"/>
            <field name="mail_ack_report_subject" colspan="4"/>
            <separator string="Email body of Acknowledgment of results report"
                colspan="4" id="mail_ack_report_body"/>
            <field name="mail_ack_report_body" colspan="4"/>
        </page>
    </xpath>
</data>
